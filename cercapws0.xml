<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/" xmlns:moz="http://www.mozilla.org/2006/browser/search/">
  <ShortName>Google PWS</ShortName>
  <Description>Google Search senza personalizzazioni</Description>
  <InputEncoding>UTF-8</InputEncoding>
  <Image width="16" height="16" type="image/x-icon">https://www.google.com/favicon.ico</Image>
  <Url type="text/html" template="https://www.google.com/search">
    <Param name="q" value="{searchTerms}"></Param>
    <Param name="pws" value="0"></Param>
  </Url>
  <Url type="application/opensearchdescription+xml" rel="self" template="https://raw.githubusercontent.com/pomhey/navigaweb/refs/heads/main/cercasenzaai.xml"></Url>
</OpenSearchDescription>
